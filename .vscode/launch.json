{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "C#: CreditApi Debug",
            "type": "dotnet",
            "request": "launch",
            "projectPath": "${workspaceFolder}/CreditApi/CreditApi.csproj",
            "launchBrowser": true,
            "env": {
